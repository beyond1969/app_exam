<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".Location0">

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="50px">
        <TableRow
            android:layout_margin="5dp">
            <TextView
                android:text="이산화질소 농도 (ppm)  "/>
            <TextView
                android:id="@+id/txt_row_0_0"
                android:text=""/>
        </TableRow>
        <TableRow
            android:layout_margin="5dp">
            <TextView
                android:text="오존 농도 (ppm)  "/>
            <TextView
                android:id="@+id/txt_row_0_1"
                android:text=""/>
        </TableRow>
        <TableRow
            android:layout_margin="5dp">
            <TextView
                android:text="일산화탄소 농도 (ppm)  "/>
            <TextView
                android:id="@+id/txt_row_0_2"
                android:text=""/>
        </TableRow>
        <TableRow
            android:layout_margin="5dp">
            <TextView
                android:text="이황산가스 농도 (ppm)  "/>
            <TextView
                android:id="@+id/txt_row_0_3"
                android:text=""/>
        </TableRow>
        <TableRow
            android:layout_margin="5dp">
            <TextView
                android:text="미세먼지 (㎍/㎥)  "/>
            <TextView
                android:id="@+id/txt_row_0_4"
                android:text=""/>
        </TableRow>
        <TableRow
            android:layout_margin="5dp">
            <TextView
                android:text="초미세먼지 (㎍/㎥)  "/>
            <TextView
                android:id="@+id/txt_row_0_5"
                android:text=""/>
        </TableRow>
    </TableLayout>
</android.support.constraint.ConstraintLayout>